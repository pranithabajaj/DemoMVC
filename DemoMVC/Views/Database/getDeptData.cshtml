@*@model IEnumerable<DemoMVC.Models.EMPDATA>*@
@{
    ViewBag.Title = "getDeptData";
}

<h2>getDeptData</h2>
@using (Html.BeginForm("getDept","Database"))
{
<h3>
    Enter the Deptno @Html.TextBox("txtDeptno",Request["txtDeptno"])
    <input type="submit" value="GetData"/>
</h3>
    if(ViewBag.L!=null)
    {
        <table>
            <tr>
                <td>Empno</td>
                <td>Ename</td>
                <td>Job</td>
                <td>Mgr</td>
                <td>Hiredate</td>
                <td>Sal</td>
                <td>Comm</td>
                <td>Deptno</td>
            </tr>
            @foreach(var E in ViewBag.L)
            {
                <tr>
                    <td>@E.EMPNO</td>
                    <td>@E.ENAME</td>
                    <td>@E.JOB</td>
                    <td>@E.MGR</td>
                    <td>@E.HIREDATE</td>
                    <td>@E.SAL</td>
                    <td>@E.COMM</td>
                    <td>@E.DEPTNO</td>
                </tr>
            }
        </table>
    }

}